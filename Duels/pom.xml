<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://maven.apache.org/POM/4.0.0"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <artifactId>duels</artifactId>

  <dependencies>
    <dependency>
      <artifactId>ubiquitous-bukkit</artifactId>
      <groupId>network.walrus</groupId>
      <scope>compile</scope>
      <version>${project.version}</version>
    </dependency>
    <dependency>
      <artifactId>utilities-parsing</artifactId>
      <groupId>network.walrus</groupId>
      <scope>compile</scope>
      <version>${project.version}</version>
    </dependency>
  </dependencies>

  <description>Manager for duel matches.</description>
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <artifactId>WalrusNetwork</artifactId>
    <groupId>network.walrus</groupId>
    <relativePath>../pom.xml</relativePath>
    <version>1.0.0-SNAPSHOT</version>
  </parent>

  <properties>
    <!-- Bukkit plugin name -->
    <plugin.mainClass>network.walrus.duels.DuelsPlugin</plugin.mainClass>
    <!-- The prefix to use when logging to the console -->
    <plugin.name>Duels</plugin.name>
    <!-- The main class -->
    <plugin.prefix>Duels</plugin.prefix>
  </properties>
</project>
